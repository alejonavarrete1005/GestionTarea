@model Proyecto
<h2>Detalles del Proyecto</h2>
<p><strong>Nombre:</strong> @Model.Nombre</p>
<p><strong>Descripción:</strong> @Model.Descripcion</p>
<p><strong>Fecha Inicio:</strong> @Model.FechaInicio.ToShortDateString()</p>
<p><strong>Fecha Fin:</strong> @Model.FechaFin.ToShortDateString()</p>
<a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning">Editar</a>
<a asp-action="Index" class="btn btn-secondary">Volver</a>
